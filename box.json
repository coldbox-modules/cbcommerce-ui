{
    "name":"cbcommerce-ui",
    "version":"1.0.0",
    "author":"Ortus Solutions, Corp",
    "location":"https://downloads.ortussolutions.com/ortussolutions/coldbox-modules/cbcommerce-ui/@build.version@/cbcommerce-ui-@build.version@.zip",
    "homepage":"https://cbcommerce.ortusbooks.com",
    "documentation":"https://cbcommerce.ortusbooks.com",
    "repository":{
        "type":"git",
        "URL":"https://github.com/coldbox-modules/cbcommerce-api"
    },
    "bugs":"https://github.com/coldbox-modules/cbcommerce-api/issues",
    "slug":"cbcommerce-ui",
    "shortDescription":"User interface module for the cbCommerce e-commerce module platform",
    "description":"User interface module for the cbCommerce e-commerce module platform",
    "type":"modules",
    "dependencies":{
        "cbi18n":"^2.2.0"
    },
    "installPaths":{
        "cbi18n":"modules/cbi18n/"
    },
    "scripts":{
        "harness:install":"install && cd test-harness && install"
    },
    "ignore":[
        "test-harness",
        "*/.md"
    ],
    "cfmigrations":{
        "connectionInfo":{
            "password":"${DB_PASSWORD}",
            "connectionString":"${DB_CONNECTIONSTRING}",
            "class":"${DB_CLASS}",
            "username":"${DB_USER}"
        },
        "schema":"${DB_DATABASE}",
        "defaultGrammar":"MySQLGrammar"
    }
}